<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache software fundation//dtd strts configuration 2.5//en"
"http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>
	<package name="default" extends="struts-default">
		<action name="login" class="Controller.loginAction">
			<result name="success">welcome.jsp</result>
			<result name="error">error.jsp</result>
		</action>
		<action name="Alogin" class="Controller.ALoginAction">
			<result name="success">Awelcome.jsp</result>
			<result name="error">error.jsp</result>
		</action>
		<action name="add" class="Controller.addAction">
			<result name="success">login.jsp</result>
		</action>
		<action name="update" class="Controller.update">
			<result name="update">update.jsp</result>
		</action>
		<action name="delete" class="Controller.delete">
			<result name="delete">delete.jsp</result>
		</action>
		<action name="updateOrder" class="Controller.updateOrder">
			<result name="updateOrder">updateOrder.jsp</result>
		</action>
		<action name="deleteOrder" class="Controller.deleteOrder">
			<result name="deleteOrder">deleteOrder.jsp</result>
		</action>
		<action name="finishAction" class="Controller.finishAction">
			<result name="success">finish.jsp</result>
		</action>
		
	</package>

</struts>